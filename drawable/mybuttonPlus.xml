<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:state_pressed="true" >
    <shape android:shape="rectangle"  >
      <corners android:radius="50dp" />
      <stroke android:width="1dip" android:color="#ccb800" />
      <gradient android:angle="-90" android:startColor="#ccb800" android:endColor="#ccb800"  />
    </shape>
  </item>
  <item android:state_focused="true">
    <shape android:shape="rectangle"  >
      <corners android:radius="3dip" />
      <stroke android:width="1dip" android:color="#14a76c" />
      <solid android:color="#14a76c"/>
    </shape>
  </item>
  <item >
    <shape android:shape="rectangle"  >
      <corners android:radius="50dp" />
      <stroke android:width="1dip" android:color="#e6cf00" />
      <gradient android:angle="-90" android:startColor="#e6cf00" android:endColor="#e6cf00" />
    </shape>
  </item>
</selector>